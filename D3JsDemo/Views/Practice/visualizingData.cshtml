@{
    ViewBag.Title = "visualizingData";
}

<html>
<body>

    <div id="container"></div>

    <script>

        var dataArray = [20, 30, 50]
        var svg = d3.select("#container")
                    .append("svg")
                    .attr("width", 500)
                    .attr("height", 500);

        var bar = svg.selectAll("rect")
                     .data(dataArray)
                     .enter()
                        .append("rect")
                        .attr("width", function (d) { return d * 10; })
                        .attr("height", 50)
                        .attr("y", function (d, i) { return i * 100 });

    </script>

</body>
</html>